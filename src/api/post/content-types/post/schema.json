{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "tenant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tenant.tenant",
      "inversedBy": "posts"
    },
    "response": {
      "type": "json",
      "required": true
    },
    "protocol": {
      "type": "uid",
      "required": false
    },
    "email": {
      "type": "email",
      "required": true
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "posthistories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::posthistory.posthistory",
      "mappedBy": "post"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "novo",
        "em_progresso",
        "concluido",
        "cancelado"
      ],
      "default": "novo",
      "required": false
    },
    "sensibilidade": {
      "type": "enumeration",
      "enum": [
        "baixa",
        "media",
        "alta"
      ],
      "default": "baixa"
    },
    "closedate": {
      "type": "date"
    }
  }
}
